{
  "name": "remark-gh-contributors",
  "version": "0.1.0",
  "description": "Asynchronous remark plugin to inject GitHub contributors into a markdown table",
  "license": "MIT",
  "author": {
    "name": "Vincent Weevers",
    "email": "mail@vincentweevers.nl",
    "github": "vweevers",
    "twitter": "vweevers"
  },
  "scripts": {
    "test": "standard && node test",
    "remark": "remark --use . README.md -o"
  },
  "dependencies": {
    "git-contributors": "~0.2.3",
    "mdast-util-heading-range": "~2.1.0",
    "parse-author": "~2.0.0",
    "remark-contributors": "github:vweevers/remark-contributors#custom-formatters",
    "resolve": "~1.7.1"
  },
  "devDependencies": {
    "remark-cli": "~5.0.0",
    "remark-toc": "~5.0.0",
    "standard": "~11.0.1",
    "tape": "~4.9.0"
  },
  "remarkConfig": {
    "plugins": {
      "remark-toc": {
        "maxDepth": 2,
        "tight": true
      }
    }
  },
  "keywords": [
    "contributors",
    "github",
    "inject",
    "markdown",
    "remark"
  ],
  "engines": {
    "node": ">=6"
  },
  "repository": "vweevers/remark-gh-contributors",
  "bugs": "https://github.com/vweevers/remark-gh-contributors/issues",
  "homepage": "https://github.com/vweevers/remark-gh-contributors"
}
