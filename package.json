{
  "name": "remark-git-contributors",
  "version": "3.0.0",
  "description": "remark plugin to inject Git contributors into a Markdown table",
  "license": "MIT",
  "keywords": [
    "unified",
    "remark",
    "remark-plugin",
    "plugin",
    "mdast",
    "markdown",
    "contributors",
    "github",
    "inject"
  ],
  "repository": "remarkjs/remark-git-contributors",
  "bugs": "https://github.com/remarkjs/remark-git-contributors/issues",
  "funding": {
    "type": "opencollective",
    "url": "https://opencollective.com/unified"
  },
  "author": {
    "name": "Vincent Weevers",
    "email": "mail@vincentweevers.nl",
    "github": "vweevers",
    "twitter": "vweevers"
  },
  "contributors": [
    {
      "name": "Vincent Weevers",
      "email": "mail@vincentweevers.nl",
      "github": "vweevers",
      "twitter": "vweevers"
    },
    {
      "name": "Titus Wormer",
      "email": "tituswormer@gmail.com",
      "github": "wooorm",
      "twitter": "wooorm"
    }
  ],
  "sideEffects": false,
  "type": "module",
  "main": "index.js",
  "files": [
    "formatters.js",
    "index.js"
  ],
  "dependencies": {
    "contributors-from-git": "^1.0.0",
    "deep-dot": "0.0.2",
    "mdast-util-heading-range": "^2.1.0",
    "parse-author": "^2.0.0",
    "remark-contributors": "^5.0.0",
    "resolve": "^1.8.1",
    "to-vfile": "^6.1.0",
    "vfile-find-up": "^6.0.0"
  },
  "devDependencies": {
    "c8": "^7.0.0",
    "prettier": "^2.0.0",
    "remark": "^13.0.0",
    "remark-cli": "^9.0.0",
    "remark-gfm": "^1.0.0",
    "remark-preset-wooorm": "^8.0.0",
    "tape": "^5.0.0",
    "tmpgen": "^1.0.0",
    "xo": "^0.37.0"
  },
  "scripts": {
    "format": "remark . -qfo --ignore-pattern test/ && prettier . -w --loglevel warn && xo --fix",
    "test-api": "node --conditions development test/index.js",
    "test-coverage": "c8 --check-coverage --branches 100 --functions 100 --lines 100 --statements 100 --reporter lcov npm run test-api",
    "test": "npm run format && npm run test-coverage"
  },
  "prettier": {
    "tabWidth": 2,
    "useTabs": false,
    "singleQuote": true,
    "bracketSpacing": false,
    "semi": false,
    "trailingComma": "none"
  },
  "xo": {
    "prettier": true,
    "rules": {
      "no-var": "off",
      "prefer-arrow-callback": "off"
    }
  },
  "remarkConfig": {
    "plugins": [
      "preset-wooorm",
      [
        "toc",
        {
          "heading": "contents"
        }
      ],
      "./index.js"
    ]
  }
}
